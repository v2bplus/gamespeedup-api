{
    "require": {
        "php": ">=8.0.0",
        "phpfastcache/phpfastcache": "9.1.3",
        "catfan/medoo": "2.1.*",
        "nesbot/carbon": "2.66.*",
        "guzzlehttp/guzzle": "7.7.*",
        "ramsey/uuid": "4.7.*",
        "monolog/monolog": "2.9.*",
        "vlucas/phpdotenv": "5.4.*",
        "vlucas/valitron": "1.4.*",
        "sendgrid/sendgrid": "8.0.*",
        "endroid/qr-code": "4.8.*",
        "psr/simple-cache": "2.0.*",
        "casbin/casbin": "^3.21",
        "robmorgan/phinx": "^0.13",
        "twig/twig": "^3.6",
        "gregwar/captcha": "1.*",
        "league/flysystem": "^3.15",
        "zoujingli/wechat-developer": "^1.2",
        "mjphaynes/php-resque": "4.x-dev",
        "spomky-labs/otphp": "10.0.*",
        "overtrue/wechat": "~5.0"
    },
    "require-dev": {
        "phpmd/phpmd": "@stable",
        "sentry/sentry": "3.*",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "4.4.*"
    },
    "repositories": {
        "packagist": {
            "description": "阿里云镜像",
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/",
            "canonical": false
        }
    },
    "autoload": {
        "psr-4": {
            "Services\\": "application/services",
            "Jobs\\": "application/jobs"
        }
    },
    "config": {
        "allow-plugins": {
            "easywechat-composer/easywechat-composer": false,
            "php-http/discovery": false
        }
    }
}
